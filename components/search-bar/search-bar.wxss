.search-box{
  width: 750rpx;
  height: 7%;
  border-bottom: 1px solid #eee;
}
.search{
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 30rpx auto;
  width: 710rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #999;
  font-size: 32rpx;
  background-color: #F8F8F8;
  border-radius: 12rpx;
}
.icon-search{
  margin-right: 12rpx;
  font-size: 28rpx;
}
.put-questions-footer{
  background:#5BAC00;
  position: fixed;
  bottom: 100rpx;left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100vw;
  height: 100rpx;
}
.put-questions-enter{
  display: inline-block;
  padding: 0;
  margin: 0;
  padding-left: 30rpx;
  box-sizing: border-box;
  margin-right: 20rpx;
  width: 540rpx;
  height: 50rpx;
  font-size: 26rpx;
  background-color: #F5F5F5;
  border-radius: 15rpx;
  vertical-align: middle;
}
.put-questions{
  display: inline-block;
  padding: 0;
  margin: 0;
  width: 134rpx;
  height: 54rpx;
  line-height: 54rpx;
  color: #5BAC00;
  font-size: 30rpx;
  background-color: #fff;
  border-radius: 20rpx;
  border: 1px solid #5BAC00;
  vertical-align: middle;

}