.effect-information-hd{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.weui-tab__panel{
  padding-top: 0;
}
.page-hd{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
}
.page-hd-end{
  color: gray;
  font-size: 30rpx;
}
.page-foldLineDiagram{
  margin-bottom: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  overflow: hidden;
}
.number-of-clicks-box{
  padding-bottom: 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
}
.number-of-clicks{
  font-size: 35rpx;
  font-weight: bold;
}
.radio-group{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 0;
  box-sizing: border-box;
}
.radio{
  width: 50%;
  text-align: center;
}

.myCanvas{
  width: 100%;
  height: 600rpx;
  background-color: #f4f4f4;
  z-index: 1;
}
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    -webkit-text-size-adjust:none;
    -webkit-user-select: none;
    font-size: 35rpx;
    color: #333;
    font-family: Helvetica,Arial,"Helvetica Neue","Droid Sans","Microsoft YaHei",sans-serif;
}
.canvas {
    width: 100%;
    height: 200px;
}